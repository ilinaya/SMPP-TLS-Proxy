# TLS Certificate and Key (base64 encoded)
TLS_CERT=<base64-encoded-certificate>
TLS_KEY=<base64-encoded-key>

# SMPP TLS listen port
LISTEN_PORT=3550

# Prometheus metrics listen port
METRICS_PORT=9090

# Upstream SMPP hosts in format host:port,host:port
UPSTREAM_HOSTS=10.0.0.1:2775,10.0.0.2:2775

# Log level (error, warn, info, debug, trace)
RUST_LOG=info